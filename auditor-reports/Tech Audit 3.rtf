\par status = read_prop(r, "Status") or "" 
\par total += 1 
\par if section not in summary: summary[section]=\{"total":0,"done":0\} 
\par summary[section]["total"] += 1 
\par if status.lower()=="done": 
\par summary[section]["done"] += 1 
\par done += 1 
\par remaining = total - done 
