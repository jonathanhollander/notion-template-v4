\par return r.json().get("results", []) 
\par except Exception: 
\par return [] 
\par 
\par TEXTUAL = \{"paragraph","callout","toggle","bulleted_list_item","numbered_list_item","to_do","quote","heading_1","heading_2","heading_3"\} 
\par 
\par def block_plain_text(b): 
\par t = b.get("type") 
\par if t in TEXTUAL: 
\par rt = b[t].get("rich_text", []) 
\par return "".join([seg.get("plain_text","") for seg in rt]) 
\par return "" 
\par 
\par def has_block_marker_recursive(block_id, token, max_depth=2): 
\par # Search for `token` in this block's subtree (limited depth). 
\par stack = [(block_id, 0)] 
\par while stack: 
\par bid, depth = stack.pop() 
\par for child in list_children(bid): 
\par if token in block_plain_text(child): 
\par return True 
\par if depth < max_depth: 
\par t = child.get("type") 
\par if t == "column_list": 
\par # Notion API doesn't return nested children directly; still push child id to traverse 
\par stack.append((child["id"], depth+1)) 
\par elif t in ("column","toggle","callout"): 
\par stack.append((child["id"], depth+1)) 
\par return False 
\par 
\par 
\par def ensure_synced_library(state, library_page_title="Synced Library"): 
\par # Ensure a master page that holds original synced blocks exists; return page_id. 
\par pid = state["pages"].get(library_page_title) 
\par if pid: 
\par return pid 
\par payload = \{ 
\par "parent": \{"type":"page_id","page_id": state.get("root_page_id") or state.get("root")\}, 
\par "icon": \{"type":"emoji","emoji":"}{\rtlch\fcs1 \af44\alang1037 \ltrch\fcs0 \f44\kerning0\insrsid1053922\charrsid1053922 \u-10179\'5f\u-8959\'5f}{\rtlch\fcs1 \af0\alang1037 \ltrch\fcs0 \f0\kerning0\insrsid1053922\charrsid1053922 "\}, 
\par "properties": \{"title":\{"title":[\{"type":"text","text":\{"content":library_page_title\}\}]\}\}, 
\par \} 
\par r = req("POST","https://api.notion.com/v1/pages", data=json.dumps(payload)) 
\par if expect_ok(r, "create synced library"): 
\par new_id = r.json().get("id") 
\par state["pages"][library_page_title]=new_id 
\par return new_id 
\par return None 
\par 
\par def ensure_sync_original(state, lib_pid, sync_key, content=""): 
\par # Create (or find) an original synced_block tagged with SYNC_KEY::. 
\par children = list_children(lib_pid) 
\par for b in children: 
\par if b.get("type") == "synced_block" and b["synced_block"].get("synced_from") is None: 
\par inner = list_children(b["id"]) 
\par for c in inner: 
\par if sync_key in (block_plain_text(c) or ""): 
\par return b["id"] 
\par blocks = [\{ 
\par "object":"block","type":"synced_block", 
\par "synced_block":\{"synced_from": None, "children":[ 
\par \{"object":"block","type":"callout","callout":\{ 
\par "icon":\{"type":"emoji","emoji":"}{\rtlch\fcs1 \af44\alang1037 \ltrch\fcs0 \f44\kerning0\insrsid1053922\charrsid1053922 \u-10179\'5f\u-8938\'5f}{\rtlch\fcs1 \af0\alang1037 \ltrch\fcs0 \f0\kerning0\insrsid1053922\charrsid1053922 "\}, 
\par "rich_text":[\{"type":"text","text":\{"content":f"\{sync_key\}"\}\},\{"type":"text","text":\{"content":" \bullet  Do not edit this marker text"\}\}], 
\par "color":"gray_background" 
\par \}\}, 
\par \{"object":"block","type":"paragraph","paragraph":\{"rich_text":[\{"type":"text","text":\{"content": content or "Synced original content"\}\}]\}\} 
\par ]\} 
\par \}] 
\par r = req("PATCH", f"https://api.notion.com/v1/blocks/\{lib_pid\}/children", data=json.dumps(\{"children":blocks\})) 
\par if expect_ok(r, "create sync original"): 
\par for res in r.json().get("results", []): 
\par if res.get("type") == "synced_block": 
\par return res.get("id") 
\par return None 
\par 
\par def add_synced_reference_if_absent(target_page_id, original_block_id, sync_key): 
\par # Insert a synced reference to original into target, if marker absent. 
\par if has_block_marker_recursive(target_page_id, sync_key): 
\par return True 
\par block = \{ 
\par "object":"block","type":"synced_block", 
\par "synced_block":\{"synced_from":\{"block_id": original_block_id\}\} 
\par \} 
\par r = req("PATCH", f"https://api.notion.com/v1/blocks/\{target_page_id\}/children", data=json.dumps(\{"children":[block]\})) 
\par return expect_ok(r, f"add sync ref \{sync_key\}") 
